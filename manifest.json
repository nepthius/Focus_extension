{
    "manifest_version": 2,
    "name": "Focus",
    "version": "1.0",
    "description": "Blocks out distracting sites to promote concentration",
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*", "https://www.reddit.com/*"],
            "js": ["jquery.min.js", "./main.js"],
            "css": ["index.css"]

        }
    ],
    "browser_action": {
        "default_icon": "Golden-Letter-F-Graphics-1.png",
        "default_popup": "popout.html"
        
    }

}